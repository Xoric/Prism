package xoric.prism.data;

import java.io.IOException;
import java.io.InputStream;
import java.io.OutputStream;

public class AttachmentHeader implements IPackable
{
	private final TextPacker textPacker;
	private final IntPacker intPacker;
	
	private final Text name;
	private final int start;
	private final int size;
	private final boolean isCompressed;

	public AttachmentHeader(Text name, int start, int size, boolean isCompressed)
	{
		this.name = name;
		this.start = start;
		this.size = size;
		this.isCompressed = isCompressed;
	}

	public IText_r getName()
	{
		return name;
	}

	public int getStart()
	{
		return start;
	}

	public int getSize()
	{
		return size;
	}

	public boolean isCompressed()
	{
		return isCompressed;
	}

	@Override
	public void pack(OutputStream stream) throws IOException
	{
		// TODO Auto-generated method stub
		
	}

	@Override
	public void unpack(InputStream stream) throws IOException
	{
		// TODO Auto-generated method stub
		
	}

	@Override
	public int getPackedSize()
	{
		// TODO Auto-generated method stub
		return 0;
	}
}
