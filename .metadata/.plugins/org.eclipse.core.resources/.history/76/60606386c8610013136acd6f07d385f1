package xoric.prism.scene;

import xoric.prism.data.IFloatPoint_r;

public interface IRenderer
{
	public void setColor(float r, float g, float b);

	public void drawWorld(IFloatPoint_r position, float w, float h, float zFront, float zBack);

	public void drawQuad2D(float x, float y, float w, float h);
}
