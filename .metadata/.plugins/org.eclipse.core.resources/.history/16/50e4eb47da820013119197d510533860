public enum Token
{
	LOGIN

	;

	static
	{
		LOGIN.setCS(1, 0, 2);
	}

	private Minima cs;
	private Minima sc;

	private void setCS(int intMin, int floatMin, int textMin)
	{
		cs = new Minima(intMin, floatMin, textMin);
	}

	private void setSC(int intMin, int floatMin, int textMin)
	{
		sc = new Minima(intMin, floatMin, textMin);
	}

	public Minima getCS()
	{
		return cs;
	}

	public Minima getSC()
	{
		return sc;
	}
}
