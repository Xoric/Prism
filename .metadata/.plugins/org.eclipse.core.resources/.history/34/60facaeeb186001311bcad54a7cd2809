import xoric.prism.data.types.IPoint_r;

public interface ISpriteList
{
	public void setControl(ISpriteListControl control);

	public void setSpriteSize(IPoint_r tileSize);

	public void setFilenamePattern(String prefix, String suffix);
	
	public void loadAndDisplaySprites();
}
