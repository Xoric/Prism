package xoric.prism.creator.common.view;

import javax.swing.JLabel;

import xoric.prism.swing.PrismPanel;

public class AboutDialog
{
	private final JLabel aboutLabel;
	private final PrismPanel aboutPanel;

	public AboutDialog(String creatorName)
	{
		aboutLabel = new JLabel();
		aboutPanel = new PrismPanel("About " + creatorName);
	}

}
