package xoric.prism.creator.custom.control;

import xoric.prism.creator.models.model.ModelModel;

class ControlLayer
{
	protected SpriteCollectionModel model;

	public ControlLayer(SpriteCollectionModel model)
	{
		this.model = model;
	}

	public void setModel(SpriteCollectionModel model)
	{
		this.model = model;
	}
}
