package xoric.prism.data.meta;

import xoric.prism.data.exceptions.PrismException;

public interface IMetaBlockInterpreter
{
	public void startInterpretation();

	public void interpretLine(MetaBlock sourceMetaBlock, MetaLine metaLine) throws PrismException;

	public void validateInterpretation(MetaBlock sourceMetaBlock) throws PrismException;
}
