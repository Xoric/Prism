package xoric.prism.server.main;

import xoric.prism.data.time.PrismClock;

public class ServerLoop
{
	public static void run(ServerModel model)
	{
		PrismClock clock = new PrismClock();

		boolean resume = true;
		do
		{
			//			int startMs = clock.getTimeMs();

			net.update();

		}
		while (resume);
	}
}
