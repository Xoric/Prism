public enum Token
{
	LOGIN

	;
	
	private Minima csMinima;
	private Minima scMinima;

	private Token()
	{
	}

	private void setCS(int intMin, int floatMin, int textMin)
	{
		cs = new Mimima(intMin,floatMin,textMin);
	}

	private void setSC(int intMin, int floatMin, int textMin)
	{
		sc = new Mimima(intMin,floatMin,textMin);
	}
}
