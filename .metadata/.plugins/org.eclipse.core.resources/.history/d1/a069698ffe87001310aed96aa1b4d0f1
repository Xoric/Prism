package xoric.prism.creator.common.buttonpanel;

import javax.swing.JButton;

import xoric.prism.creator.common.factory.Factory;

public class ButtonPanel
{
	private final JButton addButton;
	private final JButton editButton;
	private final JButton deleteButton;

	public ButtonPanel(String dataName, boolean add, boolean edit, boolean delete)
	{
		addButton = Factory.createAddButton(this, "Add a new " + dataName + ".");

	}
}
