import java.io.File;
import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.OutputStream;
import java.util.zip.GZIPOutputStream;

public class Test
{

	public static void main(String[] args)
	{
		try
		{
			File source = new File("E:/Temp/Trash/_Test.tex");
			int n = (int) source.length();
			byte[] buf = new byte[n];
			FileInputStream in = new FileInputStream(source);
			in.read(buf);
			in.close();

			File dest = new File("E:/Temp/Test.txt");
			OutputStream out = new FileOutputStream(dest);
			GZIPOutputStream output = new GZIPOutputStream(out);
			output.write(buf);
			output.close();

			System.out.println(source.length());
			System.out.println(dest.length());

			float f = 100.0f * (float) dest.length() / (float) source.length();
			System.out.println(f + '%');
		}
		catch (Exception e)
		{
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
