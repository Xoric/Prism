package xoric.prism.data.exceptions2;

public enum UserErrorText
{
	LOCAL_GAME_FILE_CAUSED_PROBLEM("A local game file has caused a problem."), INTERNAL_PROBLEM("An internal error occured.");

	private final String text;

	private UserErrorText(String text)
	{
		this.text = text;
	}

	@Override
	public String toString()
	{
		return text;
	}
}
