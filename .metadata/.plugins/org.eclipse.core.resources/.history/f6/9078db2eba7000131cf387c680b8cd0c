package xoric.prism.creator;

import xoric.prism.scene.IScene;

public class PrismCreator
{
	private final IScene scene;

	private final PrismDrawer drawer = new PrismDrawer();

	public PrismCreator(IScene scene)
	{
		this.scene = scene;
	}

	public void start()
	{
		//		scene.createWindow(800, 480, false);
		//		scene.startLoop(this);
		drawer.start();
	}
}
