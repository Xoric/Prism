package xoric.prism.swing;

public abstract class PrismWorker implements IRunnable
{
	protected volatile float progress;
	protected volatile String text;

	public PrismWorker()
	{
		progress = 0.0f;
		text = "";
	}

	public float getProgress()
	{
		return progress;
	}

	public String getText()
	{
		return text;
	}
}
